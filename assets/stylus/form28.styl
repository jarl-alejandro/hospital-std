.consultas
  display grid
  grid-template-columns 33.3% 33.3% 33.3%
  &-number
  &-fecha
    border-top 1px solid #585757
    border-bottom 1px solid #585757
    border-left 1px solid #585757
    text-align center
    margin 0
  &-edad
    text-align center
    border 1px solid #585757
  p
    margin 0
    color #585757
.cie-container
  margin-bottom 1em
  display grid
  grid-template-columns 93% 10%
  grid-gap 1em
.filter-cie10
  text-transform uppercase
.cie-table
  th
    background primary
    color white
  tr th,
  tr td
    border 1px solid #585757
    padding 4px 12px
.table_preescipcion
  tr:last-child
    td
      border-bottom 0
.form-select
  border 1px solid #666
  border-radius 3px
  margin-bottom 1em
.menu-form
  color white
  background primary
  padding .2em 1em
  border-bottom 1px solid #666
  .title
    font-weight bold
    text-align center
    border-bottom 1px solid #666
    &-no-border
      border-bottom inhiret
  &--list
    li
      display flex
      border-bottom 1px solid #666
      justify-content space-around
      flex-wrap wrap
  &--action
    display flex
    justify-content space-between
.paging-container
  margin 10px 0
  div
    display grid
    grid-template-columns 40% 26% 40%
    justify-content center
    align-items center
    small
      text-align center

.header__form28
  display flex
  justify-content space-around
  div
    border 1px solid #6b6a6a
  label
    background primary
    color white
    display block
    border-bottom 1px solid #6b6a6a
  input
    box-shadow none !important
    border 0 !important
    color black
    text-align center

.footer__form28
  display flex
  flex-wrap wrap
  justify-content space-between
  padding 0 1em
  border-top 1.4px solid #6b6a6a
  p
    text-align center
.form__tag
  background #eee
  padding .5em 1em
  display block
.form28__list
  border-bottom 1px solid #666
  display flex
  justify-content space-around
  align-items center

.atendidos-por__form
  border 1px solid #6b6a6a
  ul
    &:first-child
      background primary
      margin 0
      color white
.atendidos__menu
  display flex
  justify-content space-around
  flex-wrap wrap
  li
    &:first-child
      width 8em
      text-align center
.form__grid_nacido
  display grid
  grid-template-columns 48% 48%
  justify-content center

.form__grid_nacido_2
  display grid
  grid-template-columns 33% 33% 33%
  justify-content center

.input__antecedentes
  border 1px solid #9e9e9e !important
  border-radius 11px !important
  text-align center
  box-shadow inherit !important
  height 2em !important
.obstretico_circle
  border 1px solid #9e9e9e !important
  border-radius 8px !important
  height 2.5em
  display flex
  justify-content center
  align-items center
.meno__1year
  margin-left 7em
  margin-bottom 1em
  position relative
  a
    border 1px solid #9e9e9e !important
    width 1em
    height 1em
    border-radius 50% !important

  &:after
    content ""
    border-left 1px solid #9e9e9e
    position absolute
    height 2.5em
    top -1.5em
    left -2.5em
  &:before
    content ""
    position absolute
    border-bottom 1px solid #9e9e9e
    top -2em
    right 3.7em
    height 3em
    width 2.5em
.obsetretico_label
  position relative
  .label
    position absolute
    top 3em
    width 9em
  div
    margin-left 15px
    display flex
    align-items center
.gemelar_previo
  a
    border 1px solid #9e9e9e !important
    width 1em
    height 1em
    border-radius 50% !important
.divider_absolute
  position absolute
  top 4em
  width 100%
#gestas_previas-container
  position relative
  &:after
    content ""
    border-bottom 1px solid #9e9e9e
    position absolute
    top 2.5em
    left 100%
    width 2.3em
  &:before
    content ""
    border-left 1px solid #9e9e9e
    position absolute
    top 1em
    left 6.2em
    height 8.2em
    transform rotate(-69deg)
#gemelar_previo_container
  position relative
  &:after
    content ""
    border-bottom 1px solid #9e9e9e
    position absolute
    width 6.3em
    left 3.7em
    top 1.5em
#partos__container
  position relative
  &:after
    content ""
    border-bottom 1px solid #9e9e9e
    position absolute
    width 3.2em
    left 7.7em
    top 1.5em

  &:before
    content ""
    border-right 1px solid #9e9e9e
    position absolute
    height 4.3em
    transform rotate(50deg)
    top -2.9em
    left 9.4em
#cesarias_container
  position relative
  &:after
    content ""
    border-top 1px solid #9e9e9e
    position absolute
    left 100%
    width 1.4em
    top 1em
  &:before
    content ""
    border-right 1px solid #9e9e9e
    position absolute
    right -1.4em
    height 4em
    bottom 2.4em

#partos_vaginales-container
  position relative
  &:before
    content ""
    border-top 1px solid #9e9e9e
    position absolute
    left 100%
    width 1.4em
    top 2em

#nacido_vivos-container
  position relative
  &:before
    content ""
    border-left 1px solid #9e9e9e
    position absolute
    height 2.9em
    transform rotate(240deg)
    top 2em
    left -1.3em

#nacido_muerto-container
  position relative
  &:before
    content ""
    border-left 1px solid #9e9e9e
    position absolute
    height 2.9em
    transform rotate(-240deg)
    top -1.5em
    left -1.2em

.divider_absolute-muertos
  left 57em
  top 9em
  width 100%
  position absolute
  display flex
  div:first-child
    margin-right 2em
#hijos_vivos-container
  position relative
  &:before
    content ""
    border-top 1px solid #9e9e9e
    width 4.2em
    right 100%
    position absolute
    top 2.5em
#muerto7menor-container
  position relative
  &:before
    content ""
    position absolute
    border-top 1px solid #9e9e9e
    right 76%
    top -1.2em
    width 4.3em
    transform rotate(34deg)

#muerto7mayor-container
  position relative
  &:before
    content ""
    position absolute
    border-top 1px solid #9e9e9e
    right 96%
    top -0.8em
    width 12.5em
    transform rotate(15deg)
.checkedContainer1Year
  position absolute
  top -2.5em
  left 1.3em
.grid-dinamic
  display grid
  grid-template-columns 49% 49%
  grid-gap .5em

.nacido__label
  font-size 12px !important
#partos-wrapper
  left 35.4em
#partos-vaginales-wrapper
  left 47em
@media (max-width: 800px)
  .form__grid_nacido_2
    grid-template-columns 48% 48%
  .form__grid_nacido
    grid-template-columns 100%

  .meno__1year:before
    content ""
    right 2.2em
    width 2.4em
  #gestas_previas-container:after
    content ""
    width 3.3em
  #gemelar_previo_container:after
    content ""
    width 7.9em
    left 2.1em
  #partos__container
    &:after
      content ""
      width 4.1em
      left 6.8em
    &:before
      height 5.3em
      top -3.5em
      left 8.9em
  #cesarias_container:before
    content ""
    height 4.1em
  #partos-vaginales-wrapper
    left 46em
  #muerto7mayor-container:before
    content ""
    width 11em
  #hijos_vivos-container:before
    width 6.1em
  #muerto7menor-container:before
    right 106%

@media (max-width: 732px)
  .grid-dinamic
    grid-template-columns 100%
    grid-gap 0
  #gestas_previas-container
    label
      font-size: 10px
  .meno__1year:before
    right 1.9em
    width 2.5em
  #gemelar_previo_container:after
    width 8em
    left 2em
  #gestas_previas-container:after
    width 4em
  #partos-wrapper
    left 34.1em
  #partos__container
    &:before
      height 4.7em
      top -3.4em
      left 7.9em
    &:after
      width 3.5em
      left 6.2em
  #hijos_vivos-container:before
    width 6.5em
  #muerto7menor-container:before
    right 102%
    width 4.6em
  #muerto7mayor-container:before
    width: 13.3em;

@media (max-width: 640px)
  .meno__1year:before
    right 1.7em
    width 2.5em
  #gestas_previas-container
    label
      font-size 9px
  #gemelar_previo_container:after
    content ""
    left 1.8em
    width: 8.1em
  #gestas_previas-container:after
    width 4.3em
  #partos__container
    &:after
      left 5.6em
      width 5.3em
    &:before
      height 6.6em
      top -3.6em
      left 8.2em
      transform rotate(55deg)
  #partos-vaginales-wrapper
    left 46.7em
  #hijos_vivos-container:before
    width: 6.6em;
  #muerto7menor-container:before
    right 111%
  #muerto7mayor-container:before
    width 12.7em
  #partos-wrapper
    left 35.4em

@media (max-width: 412px)
