    grid-template-columns 100%
    &-number
      border-right 1px solid #585757
    &-fecha
      border-right 1px solid #585757
      border-top 0
    &-edad
      border 1px solid #585757
      border-top 0

  .meno__1year:before
    right 3.3em
    width 2.5em
  #partos-wrapper
    left 32.8em
  #gestas_previas-container:after
    width 2.2em
  #gemelar_previo_container:after
    left 2em
    width 8em
  #partos__container
    &:after
      left 4.6em
      width 3.8em
    &:before
      height 5.1em
      top -2.6em
      left 6.7em
  #partos-vaginales-wrapper
    left 45.7em
  #hijos_vivos-container:before
    width 5.2em
  #muerto7mayor-container:before
    width 11.4em
  #muerto7menor-container:before
    right 88%
@media (max-width: 360px)
  .grid-dinamic
  .form__grid_nacido
  .form__grid_nacido_2
    grid-template-columns 100%
  .grid-dinamic
    grid-gap 0
  .atendido-container
    margin-top 1em
  .nacido__label
    font-size 11px !important
  .meno__1year:before
    right 2.7em
  #gestas_previas-container
    label
      font-size 10px
  #gemelar_previo_container:after
    content ""
    left 1.8em
    width: 8.1em
  #gestas_previas-container:after
    width 3.3em
  #partos__container:after
    left 3.8em
    width 6em
  #partos__container:before
    height 6.6em
    top -4em
    left 6.8em
    transform rotate(64deg)
  #partos-wrapper
    left 34.1em
  #hijos_vivos-container:before
    width 6.4em
  #muerto7menor-container:before
    right 106%

@media (max-width: 320px)
  .meno__1year:before
    right 2.3em
  #partos__container:after
    left 3.3em
    width 7.5em
  #partos__container:before
    height 7.9em
    top -4.2em
    left 7.1em
  #hijos_vivos-container:before
    width 7.4em
  #muerto7menor-container:before
    right 124%
